pipeline {
 agent any

 environment {
   PROJECT = "CI_DEMO"
 }

 stages {

  stage('Checkout') {
    steps {
      git branch: 'main',
      url: 'https://github.com/pulipatitejashwini/devops_day-8_worklog.git'
    }
  }

  stage('Install Dependencies') {
    steps {
      sh 'echo No dependencies for static project'
    }
  }

  stage('Validation Test') {
    steps {
      sh 'chmod +x test.sh'
      sh './test.sh'
    }
  }

 }
}

